<?xml version="1.1" encoding="UTF-8"?>
<ehcache name="shirocache">
	<diskStore path="java.io.tmpdir" />
	<defaultCache 
		maxElementsInMemory="2000" 
		eternal="false"
		timeToIdleSeconds="20" 
		timeToLiveSeconds="20" 
		overflowToDisk="true" />
	<cache 
		name="emp" 
		maxElementsInMemory="2000"
		eternal="false" 
		timeToIdleSeconds="200" 
		timeToLiveSeconds="200"
		overflowToDisk="true">
	</cache>
</ehcache>